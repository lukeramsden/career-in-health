{
  "extends": [
    "airbnb",
    "plugin:vue/recommended"
  ],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2017,
    "allowImportExportEverywhere": true
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "max-len": [
      "error",
      100
    ],
    "curly": "off",
    "semi": [
      "error",
      "always"
    ],
    "semi-style": "off",
    "strict": "off",
    "indent": [
      "error",
      2,
      {
        "MemberExpression": 1
      }
    ],
    "space-in-parens": "off",
    "array-bracket-spacing": "off",
    "no-multi-spaces": "off",
    "brace-style": [
      "warn",
      "allman",
      { "allowSingleLine": true }
    ],
    "no-param-reassign": "off",
    "no-return-assign": "off",
    "no-console": "off",
    "no-multi-assign": "off",
    "implicit-arrow-linebreak": "off",
    "no-extra-boolean-cast": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allowAfterThis": true
      }
    ],
    "arrow-parens": "off",
    "func-names": "off",
    "no-new": "off",
    "computed-property-spacing": [
      "error",
      "always"
    ],
    "nonblock-statement-body-position": "off",
    "no-restricted-globals": "off",
    "comma-dangle": [
      "error",
      {
        "objects": "always-multiline",
        "arrays": "ignore",
        "imports": "ignore",
        "functions": "ignore"
      }
    ],
    "vue/max-attributes-per-line": [
      "warning",
      3
    ],
    "vue/mustache-interpolation-spacing": "off",
    "vue/html-self-closing": [
      "error",
      {
        "html": {
          "void": "any",
          "normal": "any",
          "component": "always"
        },
        "svg": "always",
        "math": "always"
      }
    ],
    "vue/require-default-prop": "off"
  },
  "globals": {
    "$": false,
    "jQuery": false,
    "_": false,
    "store": false,
    "moment": false,
    "lity": false,
    "mime": false,
    "filesize": false,
    "changeCase": false,
    "fuzzaldrin": false,
    "io": false,
    "axios": false,
    "toastr": true,
    "route": false,
    "Echo": false
  }
}