{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "akaunting/money": "^1.0",
        "appstract/laravel-blade-directives": "^1.0",
        "barryvdh/laravel-snappy": "^0.4.1",
        "calebporzio/onboard": "^1.1",
        "fedeisas/laravel-mail-css-inliner": "^2.2",
        "fideloper/proxy": "~4.0",
        "grimthorr/laravel-toast": "^1.1",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "mikehaertl/php-pdftk": "^0.4.4",
        "predis/predis": "^1.1",
        "radic/blade-extensions": "^7.0",
        "rukhsar/laravel-active-route": "^1.4",
        "spatie/laravel-medialibrary": "^6.0.0",
        "tightenco/ziggy": "~0.6.2",
        "watson/rememberable": "^2.0",
        "webpatser/laravel-uuid": "^3.0",
        "tom-lingham/searchy" : "2.*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "~2.3",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laracasts/generators": "^1.1",
        "mockery/mockery": "~1.0",
        "rtconner/laravel-kint": "~3.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
